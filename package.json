{
	"name": "advent-of-svelte",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite",
		"dev:expose": "vite --host",
		"build": "vite build",
		"preview": "vite preview",
		"deps:install": "pnpm i",
		"deps:update": "pnpm up",
		"deps:update-latest": "pnpm up -L",
		"check": "svelte-kit sync && svelte-check",
		"check:watch": "svelte-kit sync && svelte-check --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "husky"
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "^5.1.1",
		"@sveltejs/kit": "^2.5.4",

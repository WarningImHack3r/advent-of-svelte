		"svelte": "^4.2.12",
		"svelte-check": "^3.6.7",
		"svelte-confetti": "^1.3.2",
		"svelte-dnd-action": "^0.9.41",
		"svelte-headless-table": "^0.18.2",
		"svelte-sonner": "^0.3.19",
		"tailwind-merge": "^2.2.2",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.1",
		"tailwindcss-3d": "^1.0.5",

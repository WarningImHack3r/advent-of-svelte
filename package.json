		"svelte-dnd-action": "^0.9.52",
		"svelte-headless-table": "^0.18.2",
		"svelte-sonner": "^0.3.28",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.14",
		"tailwindcss-3d": "^1.0.7",
		"tslib": "^2.8.0",
		"typescript": "^5.6.3",

		"@total-typescript/ts-reset": "^0.6.1",
		"@unovis/svelte": "^1.4.4",
		"@vercel/speed-insights": "^1.0.12",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^0.21.16",
		"clsx": "^2.1.1",
		"cmdk-sv": "^0.0.18",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.45.1",
		"globals": "^15.11.0",
		"husky": "^9.1.6",

{
	"name": "advent-of-svelte",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite",
		"dev:expose": "vite --host",
		"build": "vite build",
		"preview": "vite preview",
		"deps:install": "pnpm i",
		"deps:update": "pnpm up",
		"deps:update-latest": "pnpm up -L",
		"check": "svelte-kit sync && svelte-check",
		"check:watch": "svelte-kit sync && svelte-check --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "husky"
	},
	"devDependencies": {
		"@neoconfetti/svelte": "^2.2.1",
		"@sveltejs/adapter-vercel": "^5.6.1",
		"@sveltejs/kit": "^2.17.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/vite": "^4.0.5",
		"@tanstack/table-core": "^8.20.5",
		"@total-typescript/ts-reset": "^0.6.1",
		"@unovis/svelte": "^1.5.0",
		"@vercel/speed-insights": "^1.1.0",
		"bits-ui": "1.0.0-next.90",
		"clsx": "^2.1.1",
		"eslint": "^9.20.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"husky": "^9.1.7",
		"lucide-svelte": "^0.475.0",
		"mode-watcher": "^0.5.1",
		"prettier": "^3.5.0",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.19.9",
		"svelte-check": "^4.1.4",
		"svelte-dnd-action": "^0.9.57",
		"svelte-sonner": "^0.3.28",
		"tailwind-merge": "^3.0.1",
		"tailwind-variants": "^0.3.1",
		"tailwindcss": "^4.0.5",
		"tailwindcss-animate": "^1.0.7",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.23.0",
		"vite": "^6.1.0",
		"vite-plugin-lucide-preprocess": "^1.2.4"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@vercel/speed-insights",
			"esbuild",
			"maplibre-gl"
		]
	},
	"type": "module",
	"packageManager": "pnpm@10.2.1"
}

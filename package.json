		"svelte": "^4.2.18",
		"svelte-check": "^3.8.4",
		"svelte-dnd-action": "^0.9.49",
		"svelte-headless-table": "^0.18.2",
		"svelte-sonner": "^0.3.25",
		"tailwind-merge": "^2.4.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.4",
		"tailwindcss-3d": "^1.0.6",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",

{
	"name": "advent-of-svelte",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite",
		"dev:expose": "vite --host",
		"build": "vite build",
		"preview": "vite preview",
		"deps:install": "pnpm i",
		"deps:update": "pnpm up",
		"deps:update-latest": "pnpm up -L",
		"check": "svelte-kit sync && svelte-check",
